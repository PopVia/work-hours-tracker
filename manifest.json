{
  "manifest_version": 3,
  "name": "Work Hours Tracker",
  "version": "1.0",
  "description": "Track the date and amount of time worked each day.",
  "icons": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
